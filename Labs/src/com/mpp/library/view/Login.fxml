<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<GridPane alignment="CENTER" vgap="10" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mpp.library.controller.LoginController">

	<children>
		<AnchorPane prefHeight="100.0" prefWidth="463.0">
			<children>
				<Label layoutX="131.0" layoutY="13.0" prefHeight="25.0" prefWidth="153.0" text="Welcome">
					<font>
						<Font name="Aharoni Bold" size="30.0" />
					</font>
				</Label>
				<Label layoutX="41.0" layoutY="61.0" prefHeight="17.0" prefWidth="93.0" text="Username">
					<font>
						<Font name="Bookman Old Style Bold" size="14.0" />
					</font>
				</Label>
				<TextField fx:id="txtUsername" layoutX="120.0" layoutY="57.0" prefHeight="25.0" prefWidth="192.0" />
				<Label layoutX="41.0" layoutY="100.0" text="Password">
					<font>
						<Font name="Bookman Old Style" size="14.0" />
					</font>
				</Label>
				<PasswordField fx:id="txtPassword" layoutX="120.0" layoutY="88.0" prefHeight="25.0" prefWidth="192.0" />

				<Button layoutX="145.0" layoutY="130.0" onAction="#loginUser" prefHeight="25.0" prefWidth="71.0" text="Login" />

				<Label layoutX="194.0" layoutY="142.0" prefHeight="17.0" prefWidth="153.0" />
			</children>
		</AnchorPane>
	</children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
