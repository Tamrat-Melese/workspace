<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.mpp.library.controller.MainViewController">
    <TabPane tabClosingPolicy="UNAVAILABLE"
             AnchorPane.topAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.bottomAnchor="0">
        <Tab fx:id="memberTab" text="Members" disable="true" />
        <Tab fx:id="bookTab" text="Books" disable="true"/>
        <Tab fx:id="checkoutTab" text="Checkout" disable="true"/>
    </TabPane>
</AnchorPane>
