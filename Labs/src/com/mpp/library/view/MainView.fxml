<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.mpp.library.controller.MainViewController"
          prefWidth="400" prefHeight="300">
    <TabPane fx:id="tabMenu" GridPane.rowIndex="0" GridPane.columnIndex="0" GridPane.vgrow="ALWAYS" GridPane.hgrow="ALWAYS" tabClosingPolicy="UNAVAILABLE">
        <Tab fx:id="memberTab" text="Members" disable="true">
            <GridPane fx:id="menuLayout" />
        </Tab>
        <Tab fx:id="bookTab" text="Books" disable="true">
            <GridPane fx:id="bookLayout" gridLinesVisible="true"/>
        </Tab>
        <Tab fx:id="checkoutTab" text="Checkout" disable="true">
            <GridPane fx:id="checkoutLayout" />
        </Tab>
    </TabPane>
</GridPane>
